<Window x:Class="VonatAdmin.View.NewRailway"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:VonatAdmin.View"
        mc:Ignorable="d"
        Title="NewRailway" Style="{StaticResource ResourceKey=BaseWindows}">
    <Grid>
        <ComboBox x:Name="cbTo" Style="{StaticResource cbAFrom}"></ComboBox>
        <ComboBox x:Name="cbFrom" Style="{StaticResource cbATo}"></ComboBox>
        <TextBox x:Name="TbDis" Style="{StaticResource TbDis}"/>
        <Button x:Name="AddRailway" Style="{StaticResource LoginButton}" Content="Hozzáadás" Click="AddRailway_OnClick"/>
        <Button x:Name="Back" Style="{StaticResource RegisterButton}" Content="Vissza" Click="Back_OnClick"/>
        <Label Style="{StaticResource lPassword1}" Content="Hová"/>
        <Label Style="{StaticResource lUsername}" Content="Honnan"></Label>
        <Label Style="{StaticResource lPassword2}" Content="Távolság"></Label>
    </Grid>
</Window>
